<script>
  const expectedAffiliate = ["bluezy32", "rose2white", "devoraels7"];

  window.onload = () => {
    const url = window.location.href;
    if (!expectedAffiliate.some(aff => url.includes(`affiliate=${aff}`))) {
      alert("Warning: This link may not credit your ClickBank account.");
    }
  }
</script>


<script>
  const link = atob("aHR0cHM6Ly93d3cuY2xpY2tiYW5rLm5ldC9yZWRpcmVjdD9odHRwczovL2V4YW1wbGUuY29tL2FmZmlsaWF0ZT1ibHVlenkzMg==");
  function goToClickBank() {
    window.location.href = link;
  }
</script>

<button onclick="goToClickBank()">Buy Now</button>
